<h4>Rating: <%= review.rating %></h4>
<% if review.body %>
  <p><%= review.body %></p>
<% end %>

<% if user_signed_in? && review.user == current_user%>
<%= link_to 'Edit This Review', edit_museum_review_path(review.museum, review) %>
<%= link_to 'Delete This Review', museum_review_path(review), method: :delete %>
<% end %>
